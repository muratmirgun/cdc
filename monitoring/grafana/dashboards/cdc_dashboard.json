{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Watcher Event Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(cdc_watcher_events_total[1m])",
          "legendFormat": "Watcher Events"
        }
      ]
    },
    {
      "title": "Syncer Consumption Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(cdc_syncer_messages_consumed_total[1m])",
          "legendFormat": "Consumed Messages"
        }
      ]
    },
    {
      "title": "Memory Usage (RSS)",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "title": "CPU Usage (%)",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[1m]) * 100",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "title": "Goroutines",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "go_goroutines",
          "legendFormat": "{{job}}"
        }
      ]
    },
    {
      "title": "Watcher Publish Errors",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum(cdc_watcher_publish_errors_total)",
          "legendFormat": "Publish Errors"
        }
      ]
    },
    {
      "title": "Syncer Write Errors",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum(cdc_syncer_write_errors_total)",
          "legendFormat": "Write Errors"
        }
      ]
    },
    {
      "title": "Syncer Batch Writes",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 18
      },
      "targets": [
        {
          "expr": "rate(cdc_syncer_batch_writes_total[1m])",
          "legendFormat": "Batch Writes"
        }
      ]
    },
    {
      "title": "Syncer UPSERT Latency (P99)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 18
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(cdc_syncer_upsert_latency_seconds_bucket[5m])))",
          "legendFormat": "P99 Latency"
        }
      ]
    }
  ],
  "refresh": "5s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": [
    "cdc",
    "pipeline"
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "CDC Pipeline Overview",
  "uid": "cdc_pipeline",
  "version": 1,
  "weekStart": ""
}